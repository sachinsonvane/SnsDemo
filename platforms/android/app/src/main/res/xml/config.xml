<?xml version='1.0' encoding='utf-8'?>
<widget id="com.snsdemo" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="mysnsreco">
        <param name="android-package" value="com.cordova.mysnsreco.SNS" />
        <param name="onload" value="true" />
    </feature>
    <name>SnsDemo</name>
    <description>
        This is cordova application for Text to Speech and Speech to Text features.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Sachin
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="android-minSdkVersion" value="26" />
    <preference name="android-targetSdkVersion" value="28" />
</widget>
